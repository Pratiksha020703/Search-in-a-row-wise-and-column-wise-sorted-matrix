def matSearch(mat,x):
    n = len(mat)
    m = len(mat[0])
    
    res = []
    
    for i in range(n):
        for j in range(m):
            if mat[i][j] == x:
                return True
                
    return False


mat = [[3, 30, 38],
        [20, 52, 54],
        [35, 60, 69]]
        
x = 35
if matSearch(mat,x):
    print("True")
    
else:
    print("False")
